<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Mycroft</os:ShortName>
<os:Description>Mycroft</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>http://mycroftproject.com/</SearchForm>
<os:Url type="text/html" method="GET" template="http://mycroftproject.com/search-engines.html">
  <os:Param name="name" value="{searchTerms}"/>
  <os:Param name="submitform" value="Search"/>
</os:Url>
</SearchPlugin>