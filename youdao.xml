<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Youdao</os:ShortName>
<os:Description>Youdao</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>http://dict.youdao.com/</SearchForm>
<os:Url type="text/html" method="GET" template="http://dict.youdao.com/search">
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="keyfrom" value="dict.index"/>
</os:Url>
</SearchPlugin>