<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Wikipedia</os:ShortName>
<os:Description>Wikipedia</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>http://www.wikipedia.org/</SearchForm>
<os:Url type="text/html" method="GET" template="http://www.wikipedia.org/search-redirect.php">
  <os:Param name="family" value="wikipedia"/>
  <os:Param name="search" value="{searchTerms}"/>
  <os:Param name="language" value="en"/>
  <os:Param name="go" value="++%E2%86%92++"/>
  <os:Param name="go" value="Go"/>
</os:Url>
</SearchPlugin>